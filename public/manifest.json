{
  "name": "tabash",
  "description": "a bash-like 'new-tab' page for browser",
  "version": "0.8.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "commands": {
    "open-command-palette": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "Command+Shift+D"
      },
      "description": "Open command palette"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/commandPalette.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3
}